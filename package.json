{
  "name": "rosid-handler-sass",
  "version": "8.0.0",
  "description": "Load SASS and transform to CSS, add vendor prefixes and minify",
  "keywords": [
    "rosid",
    "handler",
    "transform",
    "compile",
    "sass",
    "scss",
    "postcss",
    "autoprefixer",
    "cssnano"
  ],
  "homepage": "https://github.com/electerious/rosid-handler-sass",
  "repository": {
    "type": "git",
    "url": "https://github.com/electerious/rosid-handler-sass.git"
  },
  "license": "MIT",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "eslint": "eslint \"**/*.js\"",
    "format": "npm run eslint -- --fix && npm run prettier -- --write",
    "lint": "npm run eslint && npm run prettier -- --check",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,json,md,yml}\"",
    "test": "npm run lint && node --test"
  },
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.2",
    "postcss": "^8.5.6",
    "sass": "^1.93.3"
  },
  "devDependencies": {
    "@electerious/eslint-config": "^4.1.0",
    "@electerious/prettier-config": "^3.0.0",
    "eslint": "^8.57.1",
    "fsify": "^6.0.1",
    "prettier": "^3.6.2"
  },
  "authors": [
    "Tobias Reich <tobias@electerious.com>"
  ]
}
